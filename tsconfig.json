{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"noEmit": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"esModuleInterop": true,
		"lib": ["es2018"],
		"typeRoots": ["./node_modules/@types", "./scripts/typings"]
	}
}
